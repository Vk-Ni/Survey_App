    <table>
      <thead name="survey_list" id="survey_list">
        <tr>
          <th>Survey_ID</th>
          <th>Survey_Name</th>
          <th>Survey_Creator</th>
          <th>Date of creation</th>
          <th>Active_Status</th>
        </tr>
      </thead>
      <tbody>
        {{#each rows}}
