          <td>{{this.Survey_id}}</td>
          <td>{{this.Survey_name}}</td>
          <td>{{this.Survey_id}}</td>
          <td>{{this.Survey_id}}</td>
         </tr>
        {{/each}}

        
      </tbody>
    </table>
  </div>

